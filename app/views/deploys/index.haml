- content_for :app_head do
  = javascript_include_tag "pages/deploys/index-bundle"
  = stylesheet_link_tag "pages/deploys/index-bundle", :media => "all"

- content_for :app_content do
  .wrap-div
    .form-group.btn-div
      %button.btn#deploy-production 部署production
      %button.btn#deploy-staging 部署staging
      %button.btn#deploy-development 部署development
    .tabbable
      %ul.nav.nav-tabs
        %li.active
          %a#update{"data-toggle" => "tab", :href => "#tab1"} 代码更新
        %li
          %a#bundle{"data-toggle" => "tab", :href => "#tab2"} 插件安装
      .tab-content
        #tab1.tab-pane.active
          .message-div
            %pre.message-conten#code_update
        #tab2.tab-pane
          .message-div
            %pre.message-content#bundle_message
