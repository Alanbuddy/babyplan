- content_for :app_head do
  = javascript_include_tag "pages/user_mobile/sessions/set_password-bundle"
  = stylesheet_link_tag "pages/user_mobile/sessions/set_password-bundle", :media => "all"
  :javascript
    window.uid="#{@uid}"

- content_for :app_content do
  .content-area
    %a{:href => "#"}
      %img.circlelogo{src: asset_path("register/circlelogo.png")}
    .input-box
      %form.account 
        设置密码:
        %input#password.input-name{:type => "password"}
    .input-box
      %form.account 
        重新输入:
        %input#password_confirm.input-name{:type => "password"}
    %a.next{:href => "javascript:void(0);"} 完成
 
   