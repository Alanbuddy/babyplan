- content_for :app_head do
  = javascript_include_tag "pages/staff_mobile/transfers/index-bundle"
  = stylesheet_link_tag "pages/staff_mobile/transfers/index-bundle", :media => "all"

- content_for :app_content do
  .main-div
    = render "staff_mobile/partials/header"
    .content
      .book
        .borrow-div
          %p.word#transfer_out 绘本迁出
        .return-div
          %p.word#transfer_in 绘本迁入
        .record-div
          %p.word#transfer_record 迁移记录
    .footer.row
      .col-xs-6.icon-div#book-borrow
        %a{:href => "#"}
          %img.icon.unclicked-icon{:src => "/assets/manager/borrow_normal.png"}
        %p.unclicked-word 绘本借阅
      .col-xs-6.icon-div#book-transfer
        %a{:href => "#"}
          %img.icon.clicked-icon{:src => "/assets/manager/transport_selected.png"}
        %p.clicked-word 绘本迁移


= render "staff_mobile/partials/confirm_exit"
